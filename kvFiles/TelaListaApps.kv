<TelaListaApps>
    name: "tela_lista_apps"
    MDFloatLayout:
        ScrollView:
            size_hint_y: 0.825
            # pos_hint: {"center_x": 0.5, "center_y": 0.3}
            MDList:
                size_hint_x: 0.9
                id: app_list
                
        # TELA DA BARRA LATERAL
        MDTopAppBar:
            title: "Apps Bloqueados"
            anchor_title: "left"
            pos_hint: {"top": 1}
            left_action_items: [["arrow-left", lambda x:root.update_configs()]]
            right_action_items: [["refresh", lambda x:root.refresh()], ["account", lambda x:root.bloq_info()], ["help", lambda x: root.ajuda()]]
        
        MDTextField:
            id: text_field_search
            icon_right: "magnify"
            hint_text: "Pesquisar aplicativo"
            font_size: '16dp'
            bold: True
            size_hint: 0.79, None
            pos_hint: {"center_x": 0.5, "center_y": 0.85}
            # on_text: root.search_item(self.text)
            on_text_validate: root.search_item(self.text)
        # Botão invisivel em cima do icone de lupa, ao clicar ele pesquisa o texto que foi adicionado na barra de pesquisa
        MDRaisedButton:
            opacity: 0
            size_hint: None, None
            size: 50,40
            pos_hint: {"center_x": 0.835, "center_y": 0.864}
            on_release: root.pesquisar()
        
        MDFloatingActionButton:
            icon: "plus"
            pos_hint: {"center_x": 0.9, "center_y": 0.05}
            on_release: root.add_apps()
            elevation: 4
            shadow_offset: 0, -8
            shadow_softness: 4