<Content_add_app_1>
    orientation: "vertical"
    spacing: "4dp"
    size_hint_y: None
    height: "50dp"
    MDTextField:
        id: text_field_1
        hint_text: "Nome do aplicativo"
        adaptive_height: True
        font_name: "fonts/Poppins-Medium.ttf"
        font_size: "14sp"
        halign: "center"
        on_text: root.add_url(self.text)


<Content>
    orientation: "horizontal"
    spacing: "4dp"
    size_hint_y: None
    height: "100dp"
    MDTextField:
        id: text_field_1
        input_filter: 'int'
        text: "07"
        adaptive_height: True
